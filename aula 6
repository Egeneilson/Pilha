class Stack:
    def __init__(self):
        self.stack = []

    def push(self, componentes):
        self.stack.append(componentes)

    def pop(self):
        return self.stack.pop() if self.stack else None

    def is_empty(self):
        return not self.stack

    def reverse_string(o):
    stack = Stack()
    
    [stack.push(char) for char in o]
    
    return ''.join(stack.pop() for _ in range(len(stack.stack)))

        if __name__ == "__main__":
            strings = ["Carro amarelo", "12021998", "Python melhor linguagem", "Testing 123..."]
              for string in strings:
              print("A lista original: ", string)
              print("A lista reversa: ", reverse_string(string))
              print ()
